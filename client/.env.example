# API Configuration
REACT_APP_API_URL=http://localhost:3001

# ElevenLabs Configuration (if using client-side API calls)
REACT_APP_ELEVENLABS_API_KEY=your-elevenlabs-api-key-here

# Feature Flags
REACT_APP_ENABLE_VOICE_CLONING=true
REACT_APP_ENABLE_EMOTION_ANALYSIS=true
REACT_APP_ENABLE_LOCATION_TRACKING=true
REACT_APP_ENABLE_SPEECH_TO_SPEECH=true

# Audio Configuration
REACT_APP_MAX_RECORDING_DURATION=300
REACT_APP_AUDIO_SAMPLE_RATE=44100
REACT_APP_AUDIO_CHANNELS=1

# UI Configuration
REACT_APP_THEME_MODE=light
REACT_APP_ENABLE_DARK_MODE=true

# Analytics (optional)
REACT_APP_GOOGLE_ANALYTICS_ID=your-ga-id-here

# Development Configuration
REACT_APP_DEBUG_MODE=true
REACT_APP_LOG_LEVEL=debug