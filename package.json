{
  "name": "voicevault",
  "version": "1.0.0",
  "description": "Advanced voice recognition and management system with emotion detection",
  "author": "Dustin Pennington",
  "license": "UNLICENSED",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/voicevault.git"
  },
  "scripts": {
    "start": "node dist/server/src/index.js",
    "dev": "concurrently --kill-others \"powershell -Command cd server; npm run dev\" \"powershell -Command cd client; npm run dev\"",
    "dev:server": "cd server && npm run dev",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:server && npm run build:client",
    "build:server": "cd server && npm run build",
    "build:client": "cd client && npm run build",
    "test": "npm run test:server && npm run test:client",
    "test:server": "cd server && npm test",
    "test:client": "cd client && npm test"
  },
  "keywords": [
    "voice recognition",
    "emotion detection",
    "voice analysis",
    "speaker identification",
    "audio processing"
  ],
  "contributors": [
    {
      "name": "Dustin Pennington",
      "email": "[Your email]",
      "url": "[Your website/profile]"
    }
  ],
  "copyright": "Copyright Â© 2024 Dustin Pennington. All rights reserved.",
  "engines": {
    "node": ">=18.0.0"
  },
  "workspaces": [
    "client",
    "server",
    "shared"
  ],
  "dependencies": {
    "@chakra-ui/react": "^3.20.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/lab": "^7.0.0-beta.13",
    "@mui/x-tree-view": "^8.5.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
