{
  "name": "voicevault-server",
  "version": "1.0.0",
  "description": "VoiceVault backend server with API token management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:minimal": "tsc -p tsconfig.minimal.json",
    "start": "node dist/index.js",
    "start:minimal": "node dist/minimal-server.js",
    "dev": "ts-node src/index.ts",
    "dev:minimal": "ts-node src/minimal-server.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "dependencies": {
    "express": "^4.21.2",
    "pg": "^8.13.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "dotenv": "^16.4.7",
    "uuid": "^11.0.3",
    "mongoose": "^8.9.3",
    "@tensorflow/tfjs-node": "^4.22.0",
    "google-auth-library": "^9.15.0",
    "axios": "^1.7.9",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.4.1",
    "compression": "^1.7.5",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/express": "^5.0.0",
    "@types/pg": "^8.11.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/cors": "^2.8.17",
    "@types/uuid": "^10.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.12",
    "@types/compression": "^1.7.5",
    "@types/morgan": "^1.9.9",
    "typescript": "^5.7.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.5",
    "eslint": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "rimraf": "^6.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}